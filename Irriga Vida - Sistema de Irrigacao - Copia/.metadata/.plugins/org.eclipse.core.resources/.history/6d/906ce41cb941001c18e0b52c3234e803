	String  tipo =  textTipoFertilizante.getText(); // TIPO_FERTILIZANTE		
	String  marca =  textMarcaFertilizante.getText(); // MARCA_FERT	
	String  pesoEmbalagem =  textPesoEmbalagem.getText(); // PESO_EMBALAGEM	
	String  observacao =  textObservacao.getText(); // OBSERVACAO		
	String  tipoAplicacao =  textTipoDeAplicacao.getText(); // TIPO_APLIC		
	String  classificacao =  textClassificacao.getText(); // CLASSIF*/									
	
	/**
	 * Método Construtor para receber todos os dados de um Fertilizante.
	 * @param text - variável que armazena o valor recebido do ID.
	 * @param text2 - variável que armazena o valor recebido do NOME.
	 * @param text3 - variável que armazena o valor recebido do TIPO.
	 * @param text4 - variável que armazena o valor recebido da MARCA.
	 * @param text5 - variável que armazena o valor recebido do PESO DA EMBALAGEM.
	 * @param text6 - variável que armazena o valor recebido da OBSERVAÇÃO.
	 * @param text7 - variável que armazena o valor recebido do TIPO DE APLICAÇÃO.
	 * @param text8 - variável que armazena o valor recebido da CLASSIICAÇÃO.
	 */
	public Fertilizante(String text, String text2, String text3, String text4, String text5, String text6, String text7,
			String text8) {
		this.id = text;
		this.nome = text2;
		this.tipo=text3;
		this.marca=text4;
		this.peso=text5;
		this.observacao=text6;
		this.tipoAplicacao=text7;
		this.classificacao=text8;
	}

	/**
	 * Método que recebe
	 * @return - retorna
	 */
	public String getId() {
		return id;
	}

	/**
	 * Método que define
	 * @param text - variável que armazena
	 */
	public void setId(String text) {
		this.id = text;
	}

	
	
	/**
	 * Método que recebe
	 * @return - retorna
	 */
	public String getNome() {
		return nome;
	}
	
	/**
	 * Método que define
	 * @param text2 - variável que armazena
	 */
	public void setNome(String text2) {
		this.nome = text2;
	}

	
	
	
	/**
	 * Método que recebe
	 * @return - retorna
	 */
	public String getTipo() {
		return tipo;
	}
	
	/**
	 * Método que define
	 * @param text3 - variável que armazena
	 */
	public void setTipo(String text3) {
		this.tipo = text3;
	}

	
	
	/**
	 * Método que recebe
	 * @return - retorna
	 */
	public String getMarca() {
		return marca;
	}
	
	/**
	 * Método que define
	 * @param text4 - variável que armazena
	 */
	public void setMarca(String text4) {
		this.marca = text4;
	}

	
	/**
	 * Método que recebe
	 * @return - retorna
	 */
	public String getPeso() {
		return peso;
	}
	
	/**
	 * Método que define
	 * @param text5 - variável que armazena
	 */
	public void setPeso(String text5) {
		this.peso = text5;
	}

	
	/**
	 * Método que recebe
	 * @return - retorna
	 */
	public String getObservacao() {
		return observacao;
	}
	
	/**
	 * Método que define
	 * @param text6 - variável que armazena
	 */
	public void setObservacao(String text6) {
		this.observacao = text6;
	}

	
	/**
	 * Método que recebe
	 * @return - retorna
	 */
	public String getTipoAplicacao() {
		return tipoAplicacao;
	}
	
	/**
	 * Método que define
	 * @param text7 - variável que armazena
	 */
	public void setTipoAplicacao(String text7) {
		this.tipoAplicacao = text7;
	}

	
	/**
	 * Método que recebe
	 * @return - retorna
	 */
	public String getClassificacao() {
		return classificacao;
	}
	
	/**
	 * Método que define
	 * @param text8 - variável que armazena
	 */
	public void setClassificacao(String text8) {
		this.classificacao = text8;
	}
	
	
}
