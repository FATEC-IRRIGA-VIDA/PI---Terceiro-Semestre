package metodos_projeto;

public class Usuario {

	public String id;
	public String nome;
	private String senha;
	private String email;
	private String status;
	private String perfil;
	
	
	public Usuario(String text, String text2, String text3, String text4, String statusUsuario, String statusPerfil) {
		this.id= text;
		this.nome=text2;
		this.senha=text3;
		this.email=text4;
		this.status=statusUsuario;
		this.perfil=statusPerfil;
	}


	public String getId() {
		return id;
	}


	public void setId(String text) {
		this.id = text;
	}


	public String getNome() {
		return nome;
	}


	public void setNome(String text2) {
		this.nome = text2;
	}


	public String getSenha() {
		return senha;
	}


	public void setSenha(String text3) {
		this.senha = text3;
	}


	public String getEmail() {
		return email;
	}


	public void setEmail(String text4) {
		this.email = text4;
	}


	public String getStatus() {
		return status;
	}


	public void setStatus(String statusUsuario) {
		this.status = statusUsuario;
	}


	public String getPerfil() {
		return perfil;
	}


	public void setPerfil(String statusPerfil) {
		this.perfil = statusPerfil;
	}


	
	
	
}
